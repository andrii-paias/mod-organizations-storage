{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "properties": {
    "id": {
      "type": ["integer", "null"]
    },
    "name": {
      "type": "string"
    },
    "code": {
      "type": "string"
    },
    "vendor_status": {
      "type": "string"
    },
    "language": {
      "type": "integer"
    },
    "erp_code": {
      "type": "string"
    },
    "payment_method": {
      "type": "string"
    },
    "access_provider": {
      "type": "boolean"
    },
    "governmental": {
      "type": "boolean"
    },
    "licensor": {
      "type": "boolean"
    },
    "material_supplier": {
      "type": "boolean"
    },
    "claiming_interval": {
      "type": "integer"
    },
    "discount_percent": {
      "type": "number"
    },
    "expected_activation_interval": {
      "type": "integer"
    },
    "expected_invoice_interval": {
      "type": "integer"
    },
    "renewal_activation_interval": {
      "type": "integer"
    },
    "subscription_interval": {
      "type": "integer"
    },
    "liable_for_vat": {
      "type": "boolean"
    },
    "tax_id": {
      "type": "string"
    },
    "tax_percentage": {
      "type": "number"
    },
    "vendor_names": {
      "id": "vendor_name",
      "type": "array",
      "items": {
        "type": "object",
        "$ref": "vendor_name.json"
      }
    },
    "vendor_currencies": {
      "id": "vendor_currency",
      "type": "array",
      "items": {
        "type": "object",
        "$ref": "vendor_currency.json"
      }
    },
    "vendor_interfaces": {
      "id": "vendor_interface",
      "type": "array",
      "items": {
        "type": "object",
        "$ref": "vendor_interface.json"
      }
    },
    "agreements": {
      "id": "agreement",
      "type": "array",
      "items": {
        "type": "object",
        "$ref": "agreement.json"
      }
    },
    "accounts": {
      "id": "library_vendor_acct",
      "type": "array",
      "items": {
        "type": "object",
        "$ref": "library_vendor_acct.json"
      }
    },
    "edi_info": {
      "type": "object",
      "$ref": "edi_info.json"
    },
    "job": {
      "type": "object",
      "$ref": "job.json"
    },
    "addresses": {
      "id": "vendor_address",
      "type": "array",
      "items": {
        "type": "object",
        "$ref": "vendor_address.json"
      }
    },
    "phone_numbers": {
      "id": "vendor_phone",
      "type": "array",
      "items": {
        "type": "object",
        "$ref": "vendor_phone.json"
      }
    },
    "emails": {
      "id": "vendor_email",
      "type": "array",
      "items": {
        "type": "object",
        "$ref": "vendor_email.json"
      }
    },
    "contacts": {
      "id": "vendor_contact_person",
      "type": "array",
      "items": {
        "type": "object",
        "$ref": "vendor_contact_person.json"
      }
    },
    "notes": {
      "id": "note",
      "type": "array",
      "items": {
        "type": "object",
        "$ref": "note.json"
      }
    }
  },
  "additionalProperties": false,
  "required": [
    "name"
  ]
}